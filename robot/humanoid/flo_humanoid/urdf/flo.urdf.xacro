<?xml version="1.0"?>
<robot name="lilFlo"  xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:macro name="default_inertial" params="mass">
    <inertial>
      <mass value="${mass}" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </xacro:macro>

  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="file://$(arg mesh_dir)BODY - FLO-CHEST-01.stl"/>
      </geometry>
    </visual>
  </link>
</robot>