<launch>
<arg name="prefix" default="$(env HOME)/flo_data/flo_recording"/>


<node pkg="flo_core" type="recording_manager.py" name="recording_manager"/>

<node pkg="rosbag" type="record" name="lil_flo_bag"
args="record --output-prefix $(arg prefix) --split --duration 1m
/lower_realsense/depth/image_rect_raw_relay
/lower_realsense/color/image_raw_relay
/upper_realsense/depth/image_rect_raw_relay
/upper_realsense/color/image_raw_relay
/remote_video_clean_relay
/robot_audio/audio_relay
/lower_realsense/depth/camera_info
/lower_realsense/color/camera_info
/upper_realsense/depth/camera_info
/upper_realsense/color/camera_info
/client_count
/cmd_vel_mux/active
/cmd_vel_mux/keyboard_teleop
/cmd_vel_mux/parameter_descriptions
/cmd_vel_mux/parameter_updates
/cmd_vel_mux/safety_controller
/connected_clients
/cpu_stats
/diagnostics
/diagnostics_agg
/diagnostics_toplevel_state
/face_state
/game_runner_command_opts
/game_runner_commands
/game_runner_def
/game_runner_state
/game_runner_actions
/hdd_stats
/joint_states
/keyop_vel_smoother/parameter_descriptions
/keyop_vel_smoother/parameter_updates
/keyop_vel_smoother/raw_cmd_vel
/kobuki_safety_controller/disable
/kobuki_safety_controller/enable
/kobuki_safety_controller/reset
/mem_stats
/mobile_base/commands/controller_info
/mobile_base/commands/digital_output
/mobile_base/commands/external_power
/mobile_base/commands/led1
/mobile_base/commands/led2
/mobile_base/commands/motor_power
/mobile_base/commands/reset_odometry
/mobile_base/commands/sound
/mobile_base/commands/velocity
/mobile_base/controller_info
/mobile_base/debug/raw_control_command
/mobile_base/debug/raw_data_command
/mobile_base/debug/raw_data_stream
/mobile_base/events/bumper
/mobile_base/events/button
/mobile_base/events/cliff
/mobile_base/events/digital_input
/mobile_base/events/power_system
/mobile_base/events/robot_state
/mobile_base/events/wheel_drop
/mobile_base/sensors/core
/mobile_base/sensors/dock_ir
/mobile_base/sensors/imu_data
/mobile_base/sensors/imu_data_raw
/mobile_base/version_info
/mobile_base_nodelet_manager/bond
/motor_commands
/move/cancel
/move/feedback
/move/goal
/move/result
/move/status
/move_available
/move_goal
/move_stats
/net_stats
/odom
/robotsound
/rosout
/rosout_agg
/sound_play/cancel
/sound_play/feedback
/sound_play/goal
/sound_play/result
/sound_play/status
/statistics
/tf
/tf_static
/tts/cancel
/tts/feedback
/tts/goal
/tts/result
/tts/status
/tts_utterances
/tts_state
/flo_db_updates
"
        />

    <node pkg="flo_core" type="param_saver.py" name="param_saver">
        <param name="save_location" value="~/flo_data/"/>
    </node>
</launch>
